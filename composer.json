{
    "name": "upmind/provision-provider-hello-world",
    "description": "'Hello World' example of a Provision Category and Providers",
    "type": "library",
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Upmind\\ProvisionExample\\Category\\HelloWorld\\": "src/"
        }
    },
    "require": {
        "php": "7.4 - 8.3",
        "upmind/provision-provider-base": "^3.0 || ^4.0 || dev-v5-dev"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Upmind\\ProvisionExample\\Category\\HelloWorld\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
